<p id="notice"><%= notice %></p>

<h1>Pinpin Pin Worshops</h1>

<table>
  <thead>
    <tr>
      <th>Text</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @pinpin_pin_worshops.each do |pinpin_pin_worshop| %>
      <tr>
        <td><%= pinpin_pin_worshop.text %></td>
        <td><%= link_to 'Show', pinpin_pin_worshop %></td>
        <td><%= link_to 'Edit', edit_pinpin_pin_worshop_path(pinpin_pin_worshop) %></td>
        <td><%= link_to 'Destroy', pinpin_pin_worshop, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

